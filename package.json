{
  "name": "kei-adachi0709-portfolio",
  "version": "1.0.0",
  "description": "Kei Adachi's Portfolio Website - GitHub Pages",
  "main": "index.html",  "scripts": {
    "dev": "webpack serve --mode=development --open",
    "dev:legacy": "live-server --host=localhost --port=3000 --open=/",
    "build": "npm run clean && npm run build:prod && npm run optimize",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:analyze": "cross-env ANALYZE=true npm run build:prod",
    "clean": "rimraf assets/dist",
    "css:build": "sass assets/scss/main.scss assets/css/main.css --style=compressed",
    "css:critical": "critical index.html --base . --css assets/css/main.css --target assets/css/critical.css --width 1300 --height 900 --minify",
    "css:watch": "sass --watch assets/scss/main.scss:assets/css/main.css",
    "css:lint": "stylelint assets/scss/**/*.scss --fix",
    "js:build": "webpack --mode=production",
    "js:dev": "webpack --mode=development --watch",
    "js:lint": "eslint assets/js/**/*.js --fix",
    "images:optimize": "imagemin assets/images/**/*.{jpg,jpeg,png} --out-dir=assets/images/optimized --plugin=imagemin-webp --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant",
    "images:webp": "imagemin assets/images/**/*.{jpg,jpeg,png} --out-dir=assets/images/optimized --plugin=imagemin-webp",
    "optimize": "npm run images:optimize && npm run css:critical",
    "test": "npm run test:lighthouse && npm run test:accessibility",
    "test:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html --chrome-flags=\"--headless\"",
    "test:accessibility": "axe-core http://localhost:3000",
    "test:pagespeed": "psi https://kei-adachi0709.github.io --strategy=mobile --format=cli",
    "test:seo": "seolint index.html",
    "deploy": "npm run build && gh-pages -d .",
    "deploy:test": "npm run build && serve -s . -l 8080",
    "lint": "npm run css:lint && npm run js:lint",    "format": "prettier --write . --ignore-path .gitignore",
    "validate": "html-validate index.html",
    "audit": "npm audit && npm run test:lighthouse",
    "perf": "npm run build && npm run test:lighthouse && npm run test:pagespeed",
    "health": "npm run validate && npm run lint && npm run audit",
    "predeploy": "npm run health && npm run build",
    "prestart": "npm run install",
    "start": "npm run dev",
    "check": "npm run validate && npm run lint",
    "setup": "npm install && npm run build",
    "release": "npm run predeploy && npm run deploy"
  },
  "keywords": [
    "portfolio",
    "github-pages",
    "frontend",
    "web-development",
    "responsive-design"
  ],
  "author": "Kei Adachi <kei.adachi0709@example.com>",
  "license": "MIT",  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/plugin-proposal-dynamic-import": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "autoprefixer": "^10.4.14",
    "axe-core": "^4.7.0",
    "babel-loader": "^9.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "critical": "^6.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.42.0",
    "gh-pages": "^5.0.0",
    "html-validate": "^8.0.0",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^7.0.0",
    "lighthouse": "^10.1.0",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "psi": "^4.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.0",
    "seolint": "^2.0.0",
    "serve": "^14.2.0",
    "sharp": "^0.32.1",
    "style-loader": "^3.3.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.84.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.0",
    "aos": "^2.3.4",
    "core-js": "^3.30.2",
    "swiper": "^9.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kei-Adachi0709/Kei-Adachi0709.github.io.git"
  },
  "homepage": "https://kei-adachi0709.github.io"
}
